{
  "mcpServers": {
    "shadcn": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "shadcn@latest", "mcp"]
    },
    "kubernetes": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "mcp-server-kubernetes"]
    },
    "context7": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@upstash/context7-mcp"]
    },
    "sequential-thinking": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "playwright": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@playwright/mcp@latest"]
    },
    "postgresql-mcp": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@henkey/postgres-mcp-server"]
    }
  }
