{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:all": "tsx src/test/test-runner.ts all",
    "test:quick": "tsx src/test/test-runner.ts quick",
    "test:unit": "vitest run src/**/__tests__/**/*.test.{ts,tsx}",
    "test:integration": "vitest run src/components/**/__tests__/**/*.test.{ts,tsx}",
    "test:e2e": "vitest run src/test/e2e/**/*.test.{ts,tsx}",
    "test:auth": "vitest run src/**/*auth*.test.{ts,tsx}",
    "test:team": "vitest run src/**/*team*.test.{ts,tsx}",
    "test:stores": "vitest run src/stores/**/*.test.{ts,tsx}",
    "test:services": "vitest run src/services/**/*.test.{ts,tsx}",
    "test:hooks": "vitest run src/hooks/**/*.test.{ts,tsx}",
    "test:components": "vitest run src/components/**/*.test.{ts,tsx}",
    "test:coverage:unit": "vitest run --coverage src/**/__tests__/**/*.test.{ts,tsx}",
    "test:coverage:integration": "vitest run --coverage src/components/**/__tests__/**/*.test.{ts,tsx}",
    "test:coverage:all": "vitest run --coverage --reporter=verbose",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=test-results.xml",
    "test:debug": "vitest --inspect-brk --no-coverage",
    "test:clear": "vitest run --clearCache"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.90.2",
    "axios": "^1.12.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.3",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^24.0.0",
    "lucide-react": "^0.544.0",
    "msw": "^2.2.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.7.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "vitest": "^1.3.1"
  }
}
